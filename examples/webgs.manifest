(
    #64 bit elf to boot from host
    children:(kernel:(contents:(host:stage3/stage3))
              #user program
              webgs:(contents:(host:examples/webgs)))
    # filesystem path to elf for kernel to run
    program:/webgs
    # put all the tracing arguments in subtree
    trace:t
    debugsyscalls:t
    futex_trace:t    
    fault:t
    arguments:[webgs]
    environment:(USER:bobby PWD:/)
)
